<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>GMassenger</servlet-name>
		<servlet-class>com.mess.servlet.GMassengerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GMassenger</servlet-name>
		<url-pattern>/gmassenger</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>xmppreceiver</servlet-name>
		<servlet-class>com.mess.servlet.XMPPAgentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>xmppreceiver</servlet-name>
		<url-pattern>/_ah/xmpp/message/chat/</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>emailhandler</servlet-name>
		<servlet-class>com.mess.servlet.EmailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>emailhandler</servlet-name>
		<url-pattern>/_ah/mail/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EnvSetupServlet</servlet-name>
		<servlet-class>com.mess.servlet.EnvSetupServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>EnvSetupServlet</servlet-name>
		<url-pattern>/env</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DefaultCronJobServlet</servlet-name>
		<servlet-class>com.mess.servlet.DefaultCronJobServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DefaultCronJobServlet</servlet-name>
		<url-pattern>/cron/defaultcron</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SpamCronJobServlet</servlet-name>
		<servlet-class>com.mess.servlet.SpamCronJobServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SpamCronJobServlet</servlet-name>
		<url-pattern>/cron/spamcron</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>